Proceso Conversion
	// Escriba un programa que convierta de centímetros a pulgadas.
	// Una pulgada es igual a 2.54 centímetros.
	Definir centimetro Como Entero;
	Definir resultado Como Real;
	Escribir 'ingrese la longitud en cm ';
	Leer centimetro;
	resultado <- centimetro/2.54;
	Escribir 'los cm ingrersados son','  ',resultado,' ','pulagadas';
FinProceso
